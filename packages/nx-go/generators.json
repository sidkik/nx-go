{
  "$schema": "http://json-schema.org/schema",
  "name": "nx-go",
  "version": "0.0.1",
  "generators": {
    "application": {
      "aliases": ["app"],
      "factory": "./src/generators/application/generator",
      "schema": "./src/generators/application/schema.json",
      "description": "Generate a new Go application"
    },
    "package": {
      "aliases": ["pkg"],
      "factory": "./src/generators/package/generator",
      "schema": "./src/generators/package/schema.json",
      "description": "Generate a new Go package"
    }
  }
}
